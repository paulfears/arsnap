{
    "name": "@pianity/arsnap-default-ui",
    "version": "0.0.0",
    "description": "Eslint shared config",
    "author": "Eyal Chojnowski <eyal@pianity.com>",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build:static": "tsc && vite build",
        "preview": "vite preview",
        "typecheck": "tsc --noEmit",
        "generate-graphql": "graphql-codegen",
        "lint": "prettier --loglevel warn --check src/ && eslint src/",
        "lint:fix": "prettier --loglevel warn --write src/ && eslint --fix src/"
    },
    "dependencies": {
        "@metamask/key-tree": "^3.0.1",
        "@pianity/arsnap-adapter": "workspace:*",
        "@pianity/arsnap-compat": "workspace:*",
        "arweave": "^1.10.23",
        "autoprefixer": "^10.4.4",
        "base64-js": "^1.5.1",
        "bignumber.js": "^9.0.2",
        "graphql": "^15.0.0",
        "graphql-request": "^4.2.0",
        "graphql-tag": "^2.12.6",
        "js-base64": "^3.7.2",
        "node-forge": "^1.2.1",
        "postcss": "^8.4.12",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hook-form": "^7.30.0",
        "tailwindcss": "^3.0.24"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.6.2",
        "@graphql-codegen/typescript": "^2.4.8",
        "@graphql-codegen/typescript-graphql-request": "^4.4.5",
        "@graphql-codegen/typescript-operations": "^2.3.5",
        "@pianity/eslint-config": "workspace:*",
        "@pianity/prettier-config": "workspace:*",
        "@types/node-forge": "^1.0.0",
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@vitejs/plugin-react": "^1.0.7",
        "typescript": "^4.6.3",
        "vite": "^2.8.0",
        "vite-tsconfig-paths": "^3.4.1"
    }
}
