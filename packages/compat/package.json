{
    "name": "@pianity/arsnap-compat",
    "version": "0.1.0",
    "description": "A compatibility layer for ArSnap",
    "author": "Eyal Chojnowski <eyal@pianity.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/MetaMask/snap-template.git"
    },
    "files": [
        "dist/"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc && tsc-alias",
        "clean": "rm -r dist/",
        "typecheck": "tsc --noEmit",
        "lint": "prettier --loglevel warn --check src/ && eslint src/",
        "lint:fix": "prettier --loglevel warn --write src/ && eslint --fix src/"
    },
    "dependencies": {
        "@pianity/arsnap-adapter": "*",
        "arconnect": "^0.4.1",
        "arweave": "^1.10.23"
    },
    "devDependencies": {
        "@pianity/eslint-config": "*",
        "@pianity/prettier-config": "*",
        "tsc-alias": "^1.6.3",
        "typescript": "^4.6"
    }
}
